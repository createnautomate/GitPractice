{
  "name": "ProtractorTutorial",
  "dependencies": {
    "protractor": "^7.0.0",
    "jasmine-data-provider": "^2.2.0",
    "protractor-jasmine2-html-reporter": "0.0.7"
  },
  "scripts": {
    "webdriverupdate": "./node_modules/.bin/webdriver-manager update",
    "webdriverstart": "./node_modules/.bin/webdriver-manager start",
    "protractor": "./node_modules/.bin/protractor configuration.js",
    "SmokeSuite": "./node_modules/.bin/protractor configuration.js --suite Smoke",
    "Regression": "./node_modules/.bin/protractor configuration.js --suite Regression",
    "startwebdriver": "npm run webdriverupdate && npm run webdriverstart"
  }
}
